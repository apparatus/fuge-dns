{
  "license": "MIT",
  "name": "fuge-dns",
  "version": "2.0.0",
  "author": "Peter Elger (https://github.com/pelger)",
  "contributors": [],
  "keywords": [
    "microservices",
    "fuge",
    "configuration",
    "discovery",
    "dns",
    "kubernetes"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/apparatus/fuge-dns.git"
  },
  "main": "./index.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "lint": "echo 'linting...' && spacey-standard | snazzy",
    "test": "tap 'test/**/*.test.js' 'packages/*/test/*.js' --branches=90 --statements=90 --functions=90 --lines=90",
    "check": "npm run test && npm run lint",
    "coverage": "npm run test -- --cov --coverage-report=html",
    "coveralls": "npm run test -- --cov --coverage-report=text-lcov | coveralls"
  },
  "nyc": {
    "exclude": [
      "**/test/**/*.js"
    ]
  },
  "devDependencies": {
    "coveralls": "^2.11.14",
    "dns-socket": "^1.4.2",
    "json-stringify-safe": "^5.0.1",
    "pre-commit": "^1.1.3",
    "snazzy": "^5.0.0",
    "tap": "^7.1.2"
  },
  "pre-commit": [
    "check"
  ],
  "dependencies": {
    "fuge-named": "0.0.1",
    "spacey-standard": "^4.0.0"
  }
}
